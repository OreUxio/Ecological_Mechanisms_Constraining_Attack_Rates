@article{Kalyuzhny14:_NicheNeutrality,
  title = {Niche versus {{Neutrality}}: {{A Dynamical Analysis}}.},
  volume = {184},
  copyright = {Copyright \textcopyright{} 2014 The University of Chicago},
  issn = {0003-0147},
  shorttitle = {Niche versus {{Neutrality}}},
  abstract = {Abstract Understanding the forces shaping ecological communities is of crucial importance for basic science and conservation. After 50 years in which ecological theory has focused on either stable communities driven by niche-based forces or nonstable ``neutral'' communities driven by demographic stochasticity, contemporary theories suggest that ecological communities are driven by the simultaneous effects of both types of mechanisms. Here we examine this paradigm using the longest available records for the dynamics of tropical trees and breeding birds. Applying a macroecological approach and fluctuation analysis techniques borrowed from statistical physics, we show that both stabilizing mechanisms and demographic stochasticity fail to play a dominant role in shaping assemblages over time. Rather, community dynamics in these two very different systems is predominantly driven by environmental stochasticity. Clearly, the current melding of niche and neutral theories cannot account for such dynamics. Our results highlight the need for a new theory of community dynamics integrating environmental stochasticity with weak stabilizing forces and suggest that such theory may better describe the dynamics of ecological communities than current neutral theories, deterministic niche-based theories, or recent hybrids.},
  number = {4},
  journal = {The American Naturalist},
  doi = {10.1086/677930},
  author = {Kalyuzhny, Michael and Seri, Efrat and Chocron, Rachel and Flather, Curtis H. and Kadmon, Ronen and Shnerb, Nadav M.},
  month = oct,
  year = {2014},
  pages = {439-446}
}

