}

@article{Slobodkin64:_ExperimentalPopulations,
  title = {Experimental Populations of {{Hydrida}}},
  volume = {33},
  issn = {0021-8790},
  abstract = {1. Hydrid populations in the laboratory are linearly related to food supply. 2. It may be the case that Hydra populations cannot contain, for any extended length of time, more animals than there are food particles per day consumed. 3. Chlorohydra viridissima wins in competition with Hydra littoralis in the light and in the absence of predation. Either darkness or sufficiently intense predation permits the two species to coexist. 4. Maintenance cost and population efficiency have been evaluated for Hydra and Chlorohydra populations by the procedure used in earlier studies on Daphnia. Maintenance costs are of the same order of magnitude in the various systems analysed but are not constant from system to system. 5. The value of ecological efficiency for hydrid populations is surprisingly similar to that for Daphnia populations. 6. Symbiotic algae definitely contribute to the energy income of Chlorohydra viridissima.},
  journal = {J. Anim. Ecol.},
  doi = {10.2307/2435},
  author = {Slobodkin, L. B.},
  year = {1964},
  pages = {131-148}
}

@article{Schaffer78:_HomageRed,
  title = {Homage to the {{Red Queen}}. {{I}}. {{Coevolution}} of Predators and Their Victims},
  volume = {14},
  issn = {0040-5809},
  abstract = {In this paper we develop a model to describe the coevolution of a two-species predator-victim ecosystem. Evolution among the predators acts to displace the predators' zero growth isocline, J, to the left, whereas victim evolution has the opposite effect. More important, we show that for many such systems, there is a value of J, J{${_\ast}$}, at which the rate of predator evolution exactly equals the rate of victim evolution. This value of J turns out to be a coevolutionary steady state (CSS) which is stable to perturbations. It is suggested that this analysis permits qualitative understanding of why certain real world predator-prey systems oscillate, whereas others do not.},
  number = {1},
  journal = {Theoretical Population Biology},
  doi = {10.1016/0040-5809(78)90008-4},
  author = {Schaffer, William M. and Rosenzweig, Michael L.},
  month = aug,
  year = {1978},
  pages = {135-157}
}

@article{Maynard-Smith73:_StabilityPredator-Prey,
  title = {The {{Stability}} of {{Predator}}-{{Prey Systems}}},
  volume = {54},
  issn = {0012-9658},
  abstract = {The interactions between a predator and prey species have been analyzed by computer simulation of a model in which there are discrete breeding seasons, separated by a winter during which the predator must be able to find prey at a certain minimum rate or starve. The model is intended to represent a warm-blooded vertebrate predator and its prey. The main conclusions are: (1) Coexistence of predator and prey is possible, but if so, the number of prey present will not be substantially below the equilibrium number in the absence of predators. Mutual regulation of predator and prey, with the latter substantially below the carrying capacity of the environment, is unstable. (2) Coexistence is more likely if there are differences in hunting ability between different individual predators--for example, between young and old predators. (3) Cover for the prey enables the prey species to survive the extinction of the predator, but does not make coexistence more likely.},
  number = {2},
  journal = {Ecology},
  doi = {10.2307/1934346},
  author = {{Maynard-Smith}, J. and Slatkin, M.},
  year = {1973},
  pages = {384-391}
}

