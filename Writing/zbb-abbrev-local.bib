@incollection{Slobodkin62:_EnergyAnimal,
  title = {Energy in {{Animal Ecology}}},
  volume = {1},
  abstract = {This chapter focuses on the relevance and interest of energetics to ecology. Energy is of major significance in ecological systems. Energy-rich organic sediments accumulate at a rate that is completely insignificant compared with the rate of energy fixation by green plants. This implies that the biosphere as a whole is energy limited, although individual decomposer populations may be temporarily limited by predators or other factors. The relation between energetics and the numerical properties of populations must be in terms of energy budget analysis in which the population is considered as a steady-state system through which potential energy passes. The chapter discusses the theory of energy budgets, entropy and information in ecology, Lindeman's classical study of energy-passage through a natural community and its theoretical formulation, Daphnia study of ecological energetic, and a few field studies that have any possibility of shedding relevant light on the value of ecological efficiency.},
  booktitle = {Advances in {{Ecological Research}}},
  publisher = {{Academic Press}},
  author = {Slobodkin, L. B.},
  editor = {Cragg, J. B.},
  month = jan,
