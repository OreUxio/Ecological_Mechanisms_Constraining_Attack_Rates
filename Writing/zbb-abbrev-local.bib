  title = {Heredity - {{Abstract}} of Article: {{Spatial}} Patterns of Genetic Variation Generated by Different Forms of Dispersal during Range Expansion},
  volume = {77},
  issn = {0018-067X},
  shorttitle = {Heredity - {{Abstract}} of Article},
