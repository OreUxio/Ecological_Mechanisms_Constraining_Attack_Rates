  pages = {229-251},
  file = {/home/axel/Zotero/storage/9XNK6WUH/Whitmore et al. - 1998 - Failure Inference From a Marker Process Based on a.pdf;/home/axel/Zotero/storage/TDDTQS38/A1009617814586.html}
